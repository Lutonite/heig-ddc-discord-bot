{
    "name": "heig-ddc-discord-bot",
    "version": "0.0.0",
    "private": true,
    "main": "build/src/bot.js",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "check-types": "tsc --noEmit",
        "lint": "eslint src --ext ts",
        "lint:fix": "npm run lint -- --fix",
        "dev": "npm run build && NODE_ENV=development run-p build:watch start",
        "start": "node build/src/bot.js",
        "test": "run-p lint check-types"
    },
    "license": "MIT",
    "dependencies": {
        "@discordjs/builders": "^0.12.0",
        "@discordjs/rest": "^0.3.0",
        "@sapphire/decorators": "^4.3.0",
        "@sapphire/discord.js-utilities": "^4.9.0",
        "@sapphire/fetch": "^2.3.0",
        "@sapphire/framework": "^2.4.1",
        "@sapphire/plugin-editable-commands": "^1.2.0",
        "@sapphire/plugin-hmr": "^1.2.0",
        "@sapphire/plugin-logger": "^2.2.0",
        "@sapphire/plugin-pattern-commands": "^2.2.0",
        "@sapphire/plugin-scheduled-tasks": "^2.3.0",
        "@sapphire/plugin-subcommands": "^2.2.0",
        "@sapphire/ratelimits": "^2.4.0",
        "@sapphire/result": "^1.1.0",
        "@sapphire/stopwatch": "^1.4.0",
        "@sapphire/utilities": "^3.4.0",
        "bufferutil": "^4.0.6",
        "bull": "^4.7.0",
        "dayjs": "^1.10.8",
        "dayjs-parser": "^0.9.3",
        "discord-api-types": "^0.29.0",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0",
        "erlpack": "github:discord/erlpack",
        "firebase-admin": "^10.0.2",
        "ioredis": "^4.28.5",
        "utf-8-validate": "^5.0.9",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@sapphire/ts-config": "^3.3.2",
        "@types/bull": "^3.15.8",
        "@types/node": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "eslint": "^8.10.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "resolve.exports": "^1.1.0",
        "typescript": "^4.6.2"
    },
    "engines": {
        "node": "16.x"
    },
    "imports": {
        "#src/*": "./build/src/*.js"
    }
}
