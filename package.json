{
    "name": "heig-ddc-discord-bot",
    "version": "1.2.0",
    "private": true,
    "main": "build/src/bot.js",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "check-types": "tsc --noEmit",
        "lint": "eslint src --ext ts",
        "lint:fix": "npm run lint -- --fix",
        "dev": "npm run build && NODE_ENV=development run-p build:watch start",
        "start": "node build/src/bot.js",
        "test": "run-p lint check-types",
        "release": "HUSKY=0 standard-version",
        "prepare": "husky install"
    },
    "license": "MIT",
    "dependencies": {
        "@discordjs/builders": "^0.12.0",
        "@discordjs/rest": "^0.3.0",
        "@sapphire/decorators": "^4.3.3",
        "@sapphire/discord.js-utilities": "^4.9.3",
        "@sapphire/fetch": "^2.3.0",
        "@sapphire/framework": "^2.4.1",
        "@sapphire/plugin-editable-commands": "^1.2.0",
        "@sapphire/plugin-hmr": "^1.2.0",
        "@sapphire/plugin-logger": "^2.2.0",
        "@sapphire/plugin-pattern-commands": "^2.2.0",
        "@sapphire/plugin-scheduled-tasks": "^2.3.0",
        "@sapphire/plugin-subcommands": "^2.2.0",
        "@sapphire/ratelimits": "^2.4.3",
        "@sapphire/result": "^1.1.0",
        "@sapphire/stopwatch": "^1.4.0",
        "@sapphire/utilities": "^3.6.1",
        "bufferutil": "^4.0.6",
        "bull": "^4.8.1",
        "dayjs": "^1.11.0",
        "dayjs-parser": "^0.9.3",
        "discord-api-types": "^0.30.0",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0",
        "erlpack": "github:discord/erlpack",
        "firebase-admin": "^10.0.2",
        "ioredis": "^4.28.5",
        "ms": "^2.1.3",
        "utf-8-validate": "^5.0.9",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-conventional": "^16.2.1",
        "@sapphire/ts-config": "^3.3.3",
        "@types/bull": "^3.15.8",
        "@types/ms": "^0.7.31",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.11.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-import-resolver-typescript": "^2.7.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.7",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.1",
        "resolve.exports": "^1.1.0",
        "standard-version": "^9.3.2",
        "typescript": "^4.6.3"
    },
    "engines": {
        "node": "16.x"
    },
    "imports": {
        "#src/*": "./build/src/*.js"
    }
}
